{
    "name": "aimme/laravel-dynamo",
    "description": "app for creating laravel components",
    "authors": [
        {
            "name": "Mohamed Aiman",
            "email": "muhammadhu.aiman@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "aimme/dynamo": "~3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Aimme\\Dynamo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aimme\\Dynamo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aimme\\Dynamo\\DynamoServiceProvider"
            ]
        }
    }
}
